{
  "name": "Recipe Reader",
  "description": "Auto reads recipes into the youtube description.",
  "version": "1.0",
  "manifest_version": 2,

  "icons": {
    "16": "icon16.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]


}